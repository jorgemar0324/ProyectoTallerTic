@page
@model Taller.App.Front.pages.InfoVehiculosModel
@{
}
<style>
    body {
        background: #004aad;
        background: linear-gradient(to left, #004aad, #cdd6ff);
    }
</style>
<form class="form-horizontal">
<legend>Registro Vehiculos</legend>


<div class="form-group">
    <label class="col-sm-4 control-label" for="Nombre">Propietario :</label>
    <div class="col-sm-4">
        <input id="Nombre" name="Nombre" type="text" placeholder="" class="form-control input-md" required="">
    </div>
</div><br>
<div class="form-group">
    <label class="col-md-4 control-label" for="Apellido">Placa :</label>
    <div class="col-md-4">
        <input id="Apellido" name="Apellido" type="text" placeholder="" class="form-control input-md" required="">
    </div>
</div><br>
<div class="form-group">
    <label class="col-md-4 control-label" for="IDUsuario">Marca :</label>
    <div class="col-md-4">
        <input id="IDUsuario" name="IDUsuario" type="text" placeholder="" class="form-control input-md" required="">
    </div>
</div><br>
<div class="form-group">
    <label class="col-md-4 control-label" for="Ciudad">Modelo :</label>
    <div class="col-md-4">
        <input id="Ciudad" name="Ciudad" type="text" placeholder="" class="form-control input-md" required="">
    </div>
</div><br>
<div class="form-group">
    <label class="col-md-4 control-label" for="Telefono">Pasajeros :</label>
    <div class="col-md-4">
        <input id="Telefono" name="Telefono" type="number" placeholder="" class="form-control input-md" required="">
    </div>
</div><br>
<div class="form-group">
    <label class="col-md-4 control-label" for="Direccion">Motor :</label>
    <div class="col-md-4">
        <input id="Direccion" name="Direccion" type="text" placeholder="" class="form-control input-md" required="">
    </div>
</div><br>

<div class="form-group">
    <label class="col-md-4 control-label" for="Contraseña">Pais Origen :</label>
    <div class="col-md-4">
        <input id="Contraseña" name="Contraseña" type="password" placeholder="" class="form-control input-md">
    </div>
</div><br>

<div class="col-md-4">
    <button id="Guardar" name="Guardar" class="btn btn-success">Guardar</button>
</div>

</fieldset>
</form>